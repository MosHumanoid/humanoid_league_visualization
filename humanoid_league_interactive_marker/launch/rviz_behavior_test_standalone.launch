<launch>

    <node name="map_server" pkg="map_server" type="map_server" output="screen" args="$(find bitbots_localization)/models/f031Lines.yaml"/>
    <node name="tf_map_odom" pkg="bitbots_move_base" type="tf_map_odom.py"/>

    <include file="$(find bitbots_quintic_walk)/launch/quintic_walk.launch">
        <arg name="sim" value="true"/>
    </include>

    <include file="$(find humanoid_league_interactive_marker)/launch/rviz_behavior_test.launch" />

    <include file="$(find bitbots_hcm)/launch/hcm.launch">
        <arg name="sim" value="true" />
        <arg name="wolfgang" value="true" />
    </include>

    <include file="$(find bitbots_animation_server)/launch/animation.launch" />

    <!--include file="$(find wolfgang_description)/launch/wolfgang_rviz.launch" /-->

</launch>